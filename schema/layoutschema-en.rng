   <define name="e_PlaceObject">
      <element name="PlaceObject">
         <a:documentation>Outputs a rectangular object (image, table, box or textblock).</a:documentation>
         <optional>
            <attribute name="allocate">
               <a:documentation>Determines if the area of the object is marked as »allocated«. With allocate="no", the cursor position is not changed.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Occupy space in the grid (default for grid positioning).</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Don't allocate space in the grid (default for absolute positioning).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="area">
               <a:documentation>Name of the (positioning) area, the ojbect is placed in. If no area is given, the object is placed on the main area, the page (this is the area that contains all grid cells).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="groupname">
               <a:documentation>The name of the group that gets output. When given a groupname, PlaceObject should not contain any objects.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-bottom-right-radius">
               <a:documentation>Radius of corner bottom right.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-top-right-radius">
               <a:documentation>Radius of corner top right.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-top-left-radius">
               <a:documentation>Radius of corner top left.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-bottom-left-radius">
               <a:documentation>Radius of corner bottom left.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="frame">
               <a:documentation>Draw a frame around the object. You need to supply the frame color.</a:documentation>
               <choice>
                  <value>solid</value>
                  <a:documentation xmlns="">Draw a frame around the object.</a:documentation>
                  <value>without</value>
                  <a:documentation xmlns="">Don't draw a frame around the object.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="framecolor">
               <a:documentation>The color of the frame around the object. Only makes sense in combination with the attribute 'frame'.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="background">
               <a:documentation>Fill the background of the object (full grid cells) with a color, given by backgroundcolor.</a:documentation>
               <choice>
                  <value>full</value>
                  <a:documentation xmlns="">Fill background with the given color (in backgroundcolor).</a:documentation>
                  <value>without</value>
                  <a:documentation xmlns="">Empty background</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="backgroundcolor">
               <a:documentation>Color of the background if ›background‹ is set to ›full‹.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="rulewidth">
               <a:documentation>The thickness of the frame that is drawn around the object. Only makes sense in combination with the attribute 'frame'.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="column">
               <a:documentation>If contents is a number: the grid cell from the left margin of the area. If it is a length: the absolute position from the left paper margin. If this attribute is omitted, the system tries to place the object by itself.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="keepposition">
               <a:documentation>Don't move the virtual cursor to the next free space</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Don't move the virtual cursor.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Move the virtual cursor (default).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="page">
               <a:documentation>The page (later in the PDF), the object should appear on. Number of the keyword »next« for the next page.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="origin-x">
               <a:documentation>The origin for rotation.</a:documentation>
               <choice>
                  <value>left</value>
                  <a:documentation xmlns="">The origin is at the left hand side.</a:documentation>
                  <value>center</value>
                  <a:documentation xmlns="">The origin is in the center of the object.</a:documentation>
                  <value>right</value>
                  <a:documentation xmlns="">The origin is at the right hand side.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="origin-y">
               <a:documentation>The origin for rotation (on the vertical axis)</a:documentation>
               <choice>
                  <value>top</value>
                  <a:documentation xmlns="">Rotation around a point at the top.</a:documentation>
                  <value>center</value>
                  <a:documentation xmlns="">Rotate around the vertical center.</a:documentation>
                  <value>bottom</value>
                  <a:documentation xmlns="">Rotate around a point at the bottom.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="row">
               <a:documentation>The row where the object is placed. If none given, the publisher tries to find a row by itself. You can give a number (in grid cells) or an absolute value (from top left).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="rotate">
               <a:documentation>Experimental/obsolete. Rotates the object.  The amount of movement is defined by the specified angle; if positive, the movement will be clockwise, if negative, it will be counter-clockwise.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="maxheight">
               <a:documentation>Only used for multi-page table: the maximum height of tables.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="valign">
               <a:documentation>When an object is placed on the grid and it's height is not a multiple of grid height, there is a space left on the page between the object an the next grid cell. With this attribute you can instruct the software where to place the gap.</a:documentation>
               <choice>
                  <value>top</value>
                  <a:documentation xmlns="">The object is aligned at the top.</a:documentation>
                  <value>middle</value>
                  <a:documentation xmlns="">The object is aligned so that the space at the top is the same as at the bottom.</a:documentation>
                  <value>bottom</value>
                  <a:documentation xmlns="">The object is aligned at the bottom.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="hreference">
               <a:documentation>Determines the placement of the object relative to the given column. If 'left' (which is the default), the given column is the left border of the object. If 'right', the column determines the right edge of the object.</a:documentation>
               <choice>
                  <value>left</value>
                  <a:documentation xmlns="">The object is placed in given column.</a:documentation>
                  <value>right</value>
                  <a:documentation xmlns="">The given columns determines the right edge of the border.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="vreference">
               <a:documentation>Sets the placement of the object relative to the given row. </a:documentation>
               <choice>
                  <value>bottom</value>
                  <a:documentation xmlns="">The row determines the bottom edge of the object.</a:documentation>
                  <value>top</value>
                  <a:documentation xmlns="">If 'top' (default), the given row is the top border of the object.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <oneOrMore>
            <interleave>
               <optional>
                  <ref name="e_Image"/>
               </optional>
               <optional>
                  <ref name="e_Box"/>
               </optional>
               <optional>
                  <ref name="e_Rule"/>
               </optional>
               <optional>
                  <ref name="e_Textblock"/>
               </optional>
               <optional>
                  <ref name="e_Barcode"/>
               </optional>
               <optional>
                  <ref name="e_Table"/>
               </optional>
               <optional>
                  <ref name="e_Transformation"/>
               </optional>
               <optional>
                  <ref name="e_Frame"/>
               </optional>
            </interleave>
         </oneOrMore>
      </element>
   </define>
   <define name="e_PositioningArea">
      <element name="PositioningArea">
         <a:documentation>Describes an area which contains one or more frames. Elements can be placed within these frames.</a:documentation>
         <attribute name="name">
            <a:documentation>Name of the area.</a:documentation>
         </attribute>
         <oneOrMore>
            <ref name="e_PositioningFrame"/>
         </oneOrMore>
      </element>
   </define>
   <define name="e_PositioningFrame">
      <element name="PositioningFrame">
         <a:documentation>Defines a rectangular area for objects.</a:documentation>
         <attribute name="width">
            <a:documentation>The width of the frame in grid cells.</a:documentation>
         </attribute>
         <attribute name="height">
            <a:documentation>The height of the frame in grid cells.</a:documentation>
         </attribute>
         <attribute name="row">
            <a:documentation>The row number relative to the grid.</a:documentation>
         </attribute>
         <attribute name="column">
            <a:documentation>First column of the frame, in grid cells.</a:documentation>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="e_ProcessNode">
      <element name="ProcessNode">
         <a:documentation>Executes all given nodes. The elements, that are to be executed, are given with the attribute selection.</a:documentation>
         <attribute name="select">
            <a:documentation>Selection of child elements, that are to be processed.</a:documentation>
         </attribute>
         <optional>
            <attribute name="mode">
               <a:documentation>Name of the mode. This must match the mode at the corresponding Record element. With this it is possible to have different rules for the same element.</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="e_Record">
      <element name="Record">
         <a:documentation>Contains the instructions when the publisher processes the element in the data file with the given name.
        The record matching the root element will be called by the software automatically, all further data handling must be done
      by the user.</a:documentation>
         <attribute name="element">
            <a:documentation>The name of the element the record matches.</a:documentation>
         </attribute>
         <optional>
            <attribute name="mode">
               <a:documentation>Name of the mode that matches the mode in ProcessNode.</a:documentation>
            </attribute>
         </optional>
         <oneOrMore>
            <optional>
               <ref name="e_DefineColor"/>
            </optional>
            <optional>
               <ref name="e_DefineFontfamily"/>
            </optional>
            <optional>
               <ref name="e_EmptyLine"/>
            </optional>
            <optional>
               <ref name="e_ForAll"/>
            </optional>
            <optional>
               <ref name="e_Group"/>
            </optional>
            <optional>
               <ref name="e_Hyphenation"/>
            </optional>
            <optional>
               <ref name="e_Include"/>
            </optional>
            <optional>
               <ref name="e_InsertPages"/>
            </optional>
            <optional>
               <ref name="e_LoadDataset"/>
            </optional>
            <optional>
               <ref name="e_Loop"/>
            </optional>
            <optional>
               <ref name="e_Message"/>
            </optional>
            <optional>
               <ref name="e_NewPage"/>
            </optional>
            <optional>
               <ref name="e_NextRow"/>
            </optional>
            <optional>
               <ref name="e_NextFrame"/>
            </optional>
            <optional>
               <ref name="e_Output"/>
            </optional>
            <optional>
               <ref name="e_PlaceObject"/>
            </optional>
            <optional>
               <ref name="e_ProcessNode"/>
            </optional>
            <optional>
               <ref name="e_SaveDataset"/>
            </optional>
            <optional>
               <ref name="e_SavePages"/>
            </optional>
            <optional>
               <ref name="e_SetVariable"/>
            </optional>
            <optional>
               <ref name="e_SortSequence"/>
            </optional>
            <optional>
               <ref name="e_Switch"/>
            </optional>
            <optional>
               <ref name="e_Until"/>
            </optional>
            <optional>
               <ref name="e_While"/>
            </optional>
         </oneOrMore>
      </element>
   </define>
   <define name="e_Regular">
      <element name="Regular">
         <a:documentation>The (symbolic) name of the fontface for regular text, i.e. without bold or italic.</a:documentation>
         <attribute name="fontface">
            <a:documentation>The symbolic name fo the font file.</a:documentation>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="e_Rule">
      <element name="Rule">
         <a:documentation>Draw a horizontal or vertical rule in the grid.</a:documentation>
         <attribute name="direction">
            <a:documentation>The direction of the rule.</a:documentation>
            <choice>
               <value>horizontal</value>
               <a:documentation xmlns="">Horizontal rule</a:documentation>
               <value>vertical</value>
               <a:documentation xmlns="">Vertical rule</a:documentation>
            </choice>
         </attribute>
         <optional>
            <attribute name="rulewidth">
               <a:documentation>The rule thicknes given in grid cells or as a length.</a:documentation>
            </attribute>
         </optional>
         <attribute name="length">
            <a:documentation>The length of the rule in grid cells or as an absolute length.</a:documentation>
         </attribute>
         <optional>
            <attribute name="color">
               <a:documentation>The color of the rule. Defaults to black.</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="e_SaveDataset">
      <element name="SaveDataset">
         <a:documentation>Saves an element/attribute structure to be used in the next publisher run. The contents must have a tree structure.</a:documentation>
         <attribute name="filename">
            <a:documentation>Name of the file. Example: toc</a:documentation>
         </attribute>
         <attribute name="elementname">
            <a:documentation>Name of the root element that surrounds the elements given by the child elements.</a:documentation>
         </attribute>
         <optional>
            <attribute name="select">
               <a:documentation>Alternative to giving the data structure in the child elements.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="attributes">
               <a:documentation>The variable (as an XPath expression, e.g. $foo) which contains Attribute Elements. These attributes are added to the root element.</a:documentation>
            </attribute>
         </optional>
         <oneOrMore>
            <optional>
               <ref name="e_Loop"/>
            </optional>
            <optional>
               <ref name="e_Element"/>
            </optional>
            <optional>
               <ref name="e_Makeindex"/>
            </optional>
            <optional>
               <ref name="e_SortSequence"/>
            </optional>
            <optional>
               <ref name="e_Sequence"/>
            </optional>
            <optional>
               <ref name="e_Copy-of"/>
            </optional>
            <optional>
               <ref name="e_ForAll"/>
            </optional>
         </oneOrMore>
      </element>
   </define>
   <define name="e_SavePages">
      <element name="SavePages">
         <a:documentation>Everything enclosed in SavePages is saved internally and not placed into the PDF. Useful if the output migth be discarded.</a:documentation>
         <attribute name="name">
            <a:documentation>The name of the discarded output. For later retrieval with InsertPages.</a:documentation>
         </attribute>
         <oneOrMore>
            <interleave>
               <optional>
                  <ref name="e_DefineColor"/>
               </optional>
               <optional>
                  <ref name="e_DefineFontfamily"/>
               </optional>
               <optional>
                  <ref name="e_EmptyLine"/>
               </optional>
               <optional>
                  <ref name="e_ForAll"/>
               </optional>
               <optional>
                  <ref name="e_Group"/>
               </optional>
               <optional>
                  <ref name="e_Hyphenation"/>
               </optional>
               <optional>
                  <ref name="e_Include"/>
               </optional>
               <optional>
                  <ref name="e_InsertPages"/>
               </optional>
               <optional>
                  <ref name="e_LoadDataset"/>
               </optional>
               <optional>
                  <ref name="e_Loop"/>
               </optional>
               <optional>
                  <ref name="e_Message"/>
               </optional>
               <optional>
                  <ref name="e_NewPage"/>
               </optional>
               <optional>
                  <ref name="e_NextRow"/>
               </optional>
               <optional>
                  <ref name="e_NextFrame"/>
               </optional>
               <optional>
                  <ref name="e_Output"/>
               </optional>
               <optional>
                  <ref name="e_PlaceObject"/>
               </optional>
               <optional>
                  <ref name="e_ProcessNode"/>
               </optional>
               <optional>
                  <ref name="e_SaveDataset"/>
               </optional>
               <optional>
                  <ref name="e_SavePages"/>
               </optional>
               <optional>
                  <ref name="e_SetVariable"/>
               </optional>
               <optional>
                  <ref name="e_SortSequence"/>
               </optional>
               <optional>
                  <ref name="e_Switch"/>
               </optional>
               <optional>
                  <ref name="e_Until"/>
               </optional>
               <optional>
                  <ref name="e_While"/>
               </optional>
            </interleave>
         </oneOrMore>
      </element>
   </define>
   <define name="e_SetGrid">
      <element name="SetGrid">
         <a:documentation>Set size of the grid cells. All objects are placed in the grid.</a:documentation>
         <optional>
            <attribute name="width">
               <a:documentation>The width of a grid cell. Use either width or nx, not both.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="height">
               <a:documentation>The height of a grid cell. Use either height or ny, but not both.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="nx">
               <a:documentation>Specify the number of grid cells in horizontal direction. Use either nx or width, not both.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="ny">
               <a:documentation>Set the number of grid cells in vertical direction. Give ny or height, but not both.</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="e_SetVariable">
      <element name="SetVariable">
         <a:documentation>Associates a value with a variable name. The value can be a simple value or a more complex one consisting of several elements.</a:documentation>
         <attribute name="variable">
            <a:documentation>The name of the variable that holds the contents.</a:documentation>
         </attribute>
         <optional>
            <attribute name="select">
               <a:documentation>The value of the contents.</a:documentation>
            </attribute>
         </optional>
         <oneOrMore>
            <interleave>
               <optional>
                  <ref name="e_Paragraph"/>
               </optional>
               <optional>
                  <ref name="e_Value"/>
               </optional>
               <optional>
                  <ref name="e_Copy-of"/>
               </optional>
               <optional>
                  <ref name="e_Sequence"/>
               </optional>
               <optional>
                  <ref name="e_Element"/>
               </optional>
               <optional>
                  <ref name="e_Attribute"/>
               </optional>
               <optional>
                  <ref name="e_Switch"/>
               </optional>
               <optional>
                  <ref name="e_Loop"/>
               </optional>
               <optional>
                  <ref name="e_ForAll"/>
               </optional>
               <optional>
                  <ref name="e_Tr"/>
               </optional>
               <optional>
                  <ref name="e_Td"/>
               </optional>
               <optional>
                  <ref name="e_Columns"/>
               </optional>
               <optional>
                  <ref name="e_Tablerule"/>
               </optional>
               <optional>
                  <ref name="e_SortSequence"/>
               </optional>
               <optional>
                  <ref name="e_Tablehead"/>
               </optional>
            </interleave>
         </oneOrMore>
      </element>
   </define>
   <define name="e_Sequence">
      <element name="Sequence">
         <a:documentation>Return a selection of data. Probably not useful anymore as a separate command. Don't use this anymore!</a:documentation>
         <attribute name="select">
            <a:documentation>The data to be selected.</a:documentation>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="e_SortSequence">
      <element name="SortSequence">
         <a:documentation>Sort a list.</a:documentation>
         <attribute name="select">
            <a:documentation>The data that should be sorted.</a:documentation>
         </attribute>
         <optional>
            <attribute name="removeduplicates">
               <a:documentation>If this attribute is used then it contains the name of the data-attribute that gets evaluated when duplicates are eliminated.</a:documentation>
            </attribute>
         </optional>
         <attribute name="criterium">
            <a:documentation>Name of the attribute that is used as the sort key.</a:documentation>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="e_Sub">
      <element name="Sub">
         <a:documentation>Subscript text.</a:documentation>
         <optional>
            <ref name="e_Value"/>
         </optional>
      </element>
   </define>
   <define name="e_Sup">
      <element name="Sup">
         <a:documentation>Superscript text.</a:documentation>
         <optional>
            <ref name="e_Value"/>
         </optional>
      </element>
   </define>
   <define name="e_Switch">
      <element name="Switch">
         <a:documentation>Create an if-then-else construct. The test attribute of each Case commands is evaluated until it yields true.
        The contents of the Case gets executed. If no test suceeds, the (optional) Otherwise gets executed.</a:documentation>
         <oneOrMore>
            <ref name="e_Case"/>
         </oneOrMore>
         <optional>
            <ref name="e_Otherwise"/>
         </optional>
      </element>
   </define>
   <define name="e_Stylesheet">
      <element name="Stylesheet">
         <a:documentation>Load a CSS file or define CSS rules</a:documentation>
         <optional>
            <attribute name="filename">
               <a:documentation>The filename of the CSS stylesheet including the file extension.</a:documentation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="e_Table">
      <element name="Table">
         <a:documentation>Create a table that is similar to the HTML table model.</a:documentation>
         <optional>
            <attribute name="eval">
               <a:documentation>Evaluates the given XPath expression and discards its output.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="stretch">
               <a:documentation>If the table contents is narrow and stretch has the value no, the table only uses the minimal width. If the table
          contents is wider than the value width or stretch has the value max, the table's width is the size given in the width attribute.</a:documentation>
               <choice>
                  <value>max</value>
                  <a:documentation xmlns="">Stretch the table to its given width.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">The table width is the minimum width depending on it's contents.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="width">
               <a:documentation>The maximum width of the table (in grid cells or absolute values). Defaults to the available space.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="padding">
               <a:documentation>The distance between the table border and the table contents.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="fontface">
               <a:documentation>Name of the font family for the table. If not given, the font face ‘text’ is used.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="columndistance">
               <a:documentation>Distance between two table columns.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="leading">
               <a:documentation>The distance between two rows.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-collapse">
               <a:documentation>Determine if adjacent table cells share borders. The behavior of border-collapse="collapse" is undefined when the table has columndistance or rowdistance set to a non-zero value and if the adjacent borders don't have the same width and color.</a:documentation>
               <choice>
                  <value>separate</value>
                  <a:documentation xmlns="">The borders are part of the cell and not shared with its neighbors.</a:documentation>
                  <value>collapse</value>
                  <a:documentation xmlns="">The borders of neighboring cells overlap.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <oneOrMore>
            <interleave>
               <optional>
                  <ref name="e_Tablehead"/>
               </optional>
               <optional>
                  <ref name="e_Tablefoot"/>
               </optional>
               <optional>
                  <ref name="e_Columns"/>
               </optional>
               <optional>
                  <ref name="e_Tr"/>
               </optional>
               <optional>
                  <ref name="e_Copy-of"/>
               </optional>
               <optional>
                  <ref name="e_Tablerule"/>
               </optional>
               <optional>
                  <ref name="e_Message"/>
               </optional>
               <optional>
                  <ref name="e_Switch"/>
               </optional>
               <optional>
                  <ref name="e_Loop"/>
               </optional>
               <optional>
                  <ref name="e_ForAll"/>
               </optional>
            </interleave>
         </oneOrMore>
         <optional>
            <ref name="e_Copy-of"/>
         </optional>
         <optional>
            <ref name="e_Columns"/>
         </optional>
         <optional>
            <ref name="e_ForAll"/>
         </optional>
         <optional>
            <ref name="e_Switch"/>
         </optional>
         <optional>
            <ref name="e_Tr"/>
         </optional>
         <optional>
            <ref name="e_Tablerule"/>
         </optional>
         <optional>
            <ref name="e_Tablefoot"/>
         </optional>
         <optional>
            <ref name="e_Tablehead"/>
         </optional>
      </element>
   </define>
   <define name="e_Tablefoot">
      <element name="Tablefoot">
         <a:documentation>Create a repeating table footer.</a:documentation>
         <optional>
            <attribute name="page">
               <a:documentation>This command is only valid for the given pages. The default is to place the foot on all pages.</a:documentation>
               <choice>
                  <value>all</value>
                  <a:documentation xmlns="">Place the footer on all pages.</a:documentation>
                  <value>last</value>
                  <a:documentation xmlns="">Display the table footer only on the last page.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <oneOrMore>
            <interleave>
               <optional>
                  <ref name="e_Tablerule"/>
               </optional>
               <optional>
                  <ref name="e_Copy-of"/>
               </optional>
               <optional>
                  <ref name="e_Tr"/>
               </optional>
            </interleave>
         </oneOrMore>
      </element>
   </define>
   <define name="e_Tablehead">
      <element name="Tablehead">
         <a:documentation>Create a repeating table head.</a:documentation>
         <optional>
            <attribute name="page">
               <a:documentation>The page the table head should appear on. Defaults to »all«</a:documentation>
               <choice>
                  <value>first</value>
                  <a:documentation xmlns="">Only appear on the first page.</a:documentation>
                  <value>all</value>
                  <a:documentation xmlns="">All pages. If »first« is defined, the tablehead appears on all pages but the first.</a:documentation>
                  <value>odd</value>
                  <a:documentation xmlns="">Appear on all odd pages.</a:documentation>
                  <value>even</value>
                  <a:documentation xmlns="">Appear on all even pages.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <oneOrMore>
            <interleave>
               <optional>
                  <ref name="e_Tablerule"/>
               </optional>
               <optional>
                  <ref name="e_Copy-of"/>
               </optional>
               <optional>
                  <ref name="e_Tr"/>
               </optional>
               <optional>
                  <ref name="e_Switch"/>
               </optional>
            </interleave>
         </oneOrMore>
      </element>
   </define>
   <define name="e_Td">
      <element name="Td">
         <a:documentation>Td wraps a table row, just like HTML.</a:documentation>
         <optional>
            <attribute name="backgroundcolor">
               <a:documentation>The name of the background color (if the cell should get a background).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-left">
               <a:documentation>The width (tickness) of the left border. The border is inside the cell.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-right">
               <a:documentation>The width (tickness) of the right border. The border is inside the cell.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-top">
               <a:documentation>The width (tickness) of the top border. The border is inside the cell.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-bottom">
               <a:documentation>The width (tickness) of the bottom border. The border is inside the cell.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-left-color">
               <a:documentation>The color of the left border.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-right-color">
               <a:documentation>The color of the left border.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-top-color">
               <a:documentation>The color of the top border.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-bottom-color">
               <a:documentation>The color of the bottom border.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="class">
               <a:documentation>The css class to be used for formatting the table cell (experimental).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="id">
               <a:documentation>CSS id for this paragraph.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="colspan">
               <a:documentation>The number of columns this cell spans. Defaults to 1.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="rowspan">
               <a:documentation>The number of rows for this cell. Defaults to 1.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="align">
               <a:documentation>Horizontal alignment of the cell contents. Defaults to left.</a:documentation>
               <choice>
                  <value>left</value>
                  <a:documentation xmlns="">The contents is left aligned (ragged right). This is the default.</a:documentation>
                  <value>right</value>
                  <a:documentation xmlns="">The contents of the cell is right aligend.</a:documentation>
                  <value>center</value>
                  <a:documentation xmlns="">The contents of the cell is aligned at the center, with ragged right and left margin.</a:documentation>
                  <value>justify</value>
                  <a:documentation xmlns="">Justified text with straight margins.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="valign">
               <a:documentation>The vertical alignment of the cell.</a:documentation>
               <choice>
                  <value>top</value>
                  <a:documentation xmlns="">The contents is aligend at the top edge of the cell.</a:documentation>
                  <value>middle</value>
                  <a:documentation xmlns="">The contents is vertically centered.</a:documentation>
                  <value>bottom</value>
                  <a:documentation xmlns="">The contents is aligend at the bottom edge of the cell.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="padding">
               <a:documentation>Shorthand for setting padding-top and the other values with this length.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="padding-top">
               <a:documentation>Set the inner distance (width between contents and the border) to the top edge.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="padding-bottom">
               <a:documentation>Set the inner distance (width between contents and the border) to the bottom edge.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="padding-left">
               <a:documentation>Set the inner distance (width between contents and the border) to the left edge.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="padding-right">
               <a:documentation>Set the inner distance (width between contents and the border) to the right edge.</a:documentation>
            </attribute>
         </optional>
         <zeroOrMore>
            <choice>
               <ref name="e_Barcode"/>
               <ref name="e_Box"/>
               <ref name="e_Copy-of"/>
               <ref name="e_ForAll"/>
               <ref name="e_Image"/>
               <ref name="e_Loop"/>
               <ref name="e_Paragraph"/>
               <ref name="e_Switch"/>
               <ref name="e_Table"/>
               <ref name="e_VSpace"/>
            </choice>
         </zeroOrMore>
      </element>
   </define>
   <define name="e_Tablerule">
      <element name="Tablerule">
         <a:documentation>Insert a horizontal rule in a table</a:documentation>
         <optional>
            <attribute name="rulewidth">
               <a:documentation>The width (thickness) of the rule. Defaults to 0.25pt.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="color">
               <a:documentation>The color of the rule. Defaults to black.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="start">
               <a:documentation>The first column of the rule. Defaults to 1.</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="e_Textblock">
      <element name="Textblock">
         <a:documentation>Create a rectangular piece of text.</a:documentation>
         <optional>
            <attribute name="angle">
               <a:documentation>The angle (counter clockwise) that the text gets turned.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="color">
               <a:documentation>The name of the color of the text.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="width">
               <a:documentation>Number of columns for the text. If not given, the surrounding element determines the width of the element.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="fontface">
               <a:documentation>The name of the font family. Defaults to text'.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="textformat">
               <a:documentation>The name of the text format to be applied to the text. Defaults to text.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="columns">
               <a:documentation>Number of columns in the textblock.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="columndistance">
               <a:documentation>Distance between two columns. Defaults to 3mm.</a:documentation>
            </attribute>
         </optional>
         <oneOrMore>
            <choice>
               <ref name="e_Switch"/>
               <ref name="e_Paragraph"/>
               <ref name="e_Ol"/>
               <ref name="e_Ul"/>
               <ref name="e_Copy-of"/>
               <ref name="e_Action"/>
               <ref name="e_Bookmark"/>
               <ref name="e_Value"/>
               <ref name="e_ForAll"/>
            </choice>
         </oneOrMore>
      </element>
   </define>
   <define name="e_Text">
      <element name="Text">
         <a:documentation>Create a text that can be broken across text containers or pages. To be used with Output</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="e_Switch"/>
               <ref name="e_Paragraph"/>
               <ref name="e_Ol"/>
               <ref name="e_Ul"/>
               <ref name="e_Copy-of"/>
               <ref name="e_Action"/>
               <ref name="e_Bookmark"/>
               <ref name="e_Value"/>
               <ref name="e_ForAll"/>
            </choice>
         </oneOrMore>
      </element>
   </define>
   <define name="e_Tr">
      <element name="Tr">
         <a:documentation>Tablerow</a:documentation>
         <optional>
            <attribute name="align">
               <a:documentation>Horizontal alignment of the table cells in this row.</a:documentation>
               <choice>
                  <value>left</value>
                  <a:documentation xmlns="">The contents is left aligned (ragged right). This is the default.</a:documentation>
                  <value>right</value>
                  <a:documentation xmlns="">The contents of the cell is right aligend.</a:documentation>
                  <value>center</value>
                  <a:documentation xmlns="">The contents of the cell is aligned at the center, with ragged right and left margin.</a:documentation>
                  <value>justify</value>
                  <a:documentation xmlns="">Justified text with straight margins.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="valign">
               <a:documentation>Vertical aligment of the table cells in this row.</a:documentation>
               <choice>
                  <value>top</value>
                  <a:documentation xmlns="">The objects in this row are aligned at the top.</a:documentation>
                  <value>middle</value>
                  <a:documentation xmlns="">The objects in this row are aligned at the middle axis.</a:documentation>
                  <value>bottom</value>
                  <a:documentation xmlns="">The objects in this row are aligned at the bottom.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="minheight">
               <a:documentation>Minimum rowheight in grid cells.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="backgroundcolor">
               <a:documentation>Background color of each cell in this row.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="break-below">
               <a:documentation>Allow a table break between this row and the following.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Allow a table break between this row and the following (default).</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Disable a table break between this row and the following.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="data">
               <a:documentation>Data that can be accessed via $_last_tr_data in the tablefoot and tablehead.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="sethead">
               <a:documentation>Use this line for future table heads.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Use this line for future table heads.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">No special treatment of this line (default).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="top-distance">
               <a:documentation>Todo: document</a:documentation>
            </attribute>
         </optional>
         <oneOrMore>
            <optional>
               <ref name="e_Message"/>
            </optional>
            <optional>
               <ref name="e_Td"/>
            </optional>
            <optional>
               <ref name="e_Copy-of"/>
            </optional>
            <optional>
               <ref name="e_Switch"/>
            </optional>
            <optional>
               <ref name="e_ForAll"/>
            </optional>
            <optional>
               <ref name="e_Loop"/>
            </optional>
         </oneOrMore>
      </element>
   </define>
   <define name="e_Transformation">
      <element name="Transformation">
         <a:documentation>Manipulate an object's appearance by applying a matrix. See the PDF reference 4.2.2 Common Transformations and following.</a:documentation>
         <optional>
            <attribute name="matrix">
               <a:documentation>Experimental The transformation matrix for the object. Expected is a space separated string of six values.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="origin-x">
               <a:documentation>The origin for matrix transformation. Must be left, center or right or a number from 0 to 100 (0 = left, 100 = right).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="origin-y">
               <a:documentation>The vertical origin for the matrix tranformation. Must be top, center or bottom or a number from 0 to 100 (0 = top, 100 = bottom).</a:documentation>
            </attribute>
         </optional>
         <oneOrMore>
            <interleave>
               <optional>
                  <ref name="e_Image"/>
               </optional>
               <optional>
                  <ref name="e_Box"/>
               </optional>
               <optional>
                  <ref name="e_Rule"/>
               </optional>
               <optional>
                  <ref name="e_Textblock"/>
               </optional>
               <optional>
                  <ref name="e_Barcode"/>
               </optional>
               <optional>
                  <ref name="e_Table"/>
               </optional>
               <optional>
                  <ref name="e_Frame"/>
               </optional>
               <optional>
                  <ref name="e_Transformation"/>
               </optional>
            </interleave>
         </oneOrMore>
      </element>
   </define>
   <define name="e_U">
      <element name="U">
         <a:documentation>Underline text.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="e_Action"/>
               <ref name="e_A"/>
               <ref name="e_I"/>
               <ref name="e_B"/>
               <ref name="e_Br"/>
               <ref name="e_Color"/>
               <ref name="e_HSpace"/>
               <ref name="e_U"/>
               <ref name="e_Fontface"/>
               <ref name="e_Image"/>
               <ref name="e_Sub"/>
               <ref name="e_Sup"/>
               <ref name="e_URL"/>
               <ref name="e_Value"/>
               <ref name="e_Switch"/>
               <ref name="e_Loop"/>
               <ref name="e_ForAll"/>
            </choice>
         </zeroOrMore>
      </element>
   </define>
   <define name="e_Ul">
      <element name="Ul">
         <a:documentation>Create an unordered list, just like in HTML</a:documentation>
         <optional>
            <ref name="e_Li"/>
         </optional>
      </element>
   </define>
   <define name="e_Until">
