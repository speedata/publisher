   <define name="e_U">
      <element name="U">
         <a:documentation>Underline text.</a:documentation>
         <interleave>
            <group>
               <optional>
                  <attribute name="dashed">
                     <a:documentation>Use a dashed rule instead of a solid rule.</a:documentation>
