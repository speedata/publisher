         <optional>
            <attribute name="cutmarks">
               <a:documentation>Cut marks / crop marks will be placed in the PDF. The distance
          of the marks from the imaginary center is determined by the attribute trim,
          but is at least 5mm. The length of the cut marks is 1cm. The default of this attribute
          is no, that means no cut marks will be displayed.
        </a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Show crop marks.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Don't show crop marks (default).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="trim">
               <a:documentation>The amount of bleed. Defaults to 0mm.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="mainlanguage">
               <a:documentation>The default language for text (hyphenation). You can also set the default language on the command line and locally set it at Paragraph.</a:documentation>
               <choice>
                  <value>German</value>
                  <a:documentation xmlns="">Switch to German hyphenation patterns.</a:documentation>
                  <value>English (Great Britan)</value>
                  <a:documentation xmlns="">Switch to English hyphenation patterns.</a:documentation>
                  <value>Czech</value>
                  <a:documentation xmlns=""/>
                  <value>Danish</value>
                  <a:documentation xmlns=""/>
                  <value>Dutch</value>
                  <a:documentation xmlns=""/>
                  <value>English (USA)</value>
                  <a:documentation xmlns=""/>
                  <value>Finnish</value>
                  <a:documentation xmlns=""/>
                  <value>Greek</value>
                  <a:documentation xmlns="">Monotonic greek</a:documentation>
                  <value>Ancient Greek</value>
                  <a:documentation xmlns="">Ancient Greek hyphenation patterns.</a:documentation>
                  <value>Hungarian</value>
                  <a:documentation xmlns=""/>
                  <value>Italian</value>
                  <a:documentation xmlns=""/>
                  <value>Norwegian Bokmål</value>
                  <a:documentation xmlns=""/>
                  <value>Norwegian Nynorsk</value>
                  <a:documentation xmlns=""/>
                  <value>Polish</value>
                  <a:documentation xmlns=""/>
                  <value>Portuguese</value>
                  <a:documentation xmlns=""/>
                  <value>Russian</value>
                  <a:documentation xmlns=""/>
                  <value>Serbian</value>
                  <a:documentation xmlns=""/>
                  <value>Spanish</value>
                  <a:documentation xmlns=""/>
                  <value>Swedish</value>
                  <a:documentation xmlns=""/>
                  <value>Turkish</value>
                  <a:documentation xmlns=""/>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="trace">
               <a:documentation>Draw rectangles around objects for debugging purposes.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Verbose output in the log file and the PDF.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Regular run (default).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="startpage">
               <a:documentation>Set the number of the first page.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="show-grid">
               <a:documentation>Draw the grid on the page.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Show the grid.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Don't show the grid (default).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="show-gridallocation">
               <a:documentation>Draw allocated cells with yellow and conflicts with red markers.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Show grid allocation.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Don't show the grid allocation (default).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="show-hyphenation">
               <a:documentation>Draw little marks to show all hyphenation points.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Show hyphenation points.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Don't show any hyphenation points (default).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="resetmarks">
               <a:documentation>Yes: ignore the marks file from previous run.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Ignore marks from the previous run.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Use marks from the previous run (default).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="ignoreeol">
               <a:documentation>Ignore newlines in data-xml</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Ignore newlines in data-xml</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Respect newlines in data-xml</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
