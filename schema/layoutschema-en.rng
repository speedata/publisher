   <define name="e_Pagetype">
      <element name="Pagetype">
         <a:documentation>Define a master page. A master page is chosen depending on the criterion given with the attribute »test«.</a:documentation>
         <interleave>
            <group>
               <optional>
                  <attribute name="defaultcolor">
                     <a:documentation>The default text color for this page (unless overridden in Paragraph or Textblock). Defaults to &#39;black&#39;.</a:documentation>
                  </attribute>
               </optional>
               <attribute name="name">
                  <a:documentation>Name of the master page. It is for informational purpose and as a selection for NewPage.</a:documentation>
               </attribute>
               <attribute name="test">
                  <a:documentation>If this xpath expression evaluates to true, this page is taken as a master page.</a:documentation>
               </attribute>
               <optional>
                  <ref name="e_Margin"></ref>
               </optional>
               <interleave>
                  <optional>
                     <oneOrMore>
                        <ref name="e_PositioningArea"></ref>
                     </oneOrMore>
                  </optional>
                  <optional>
                     <ref name="e_AtPageCreation"></ref>
                  </optional>
                  <optional>
                     <ref name="e_AtPageShipout"></ref>
                  </optional>
                  <optional>
                     <ref name="e_ForAll"></ref>
                  </optional>
                  <optional>
                     <ref name="e_Grid"></ref>
                  </optional>
               </interleave>
            </group>
            <ref name="foreign-nodes"></ref>
         </interleave>
