   <define name="e_Value">
      <element name="Value">
         <a:documentation>Contains a text value that is passed to the surrounding element.</a:documentation>
         <optional>
            <attribute name="select">
               <a:documentation>Value to be passed to the outer element.</a:documentation>
            </attribute>
         </optional>
         <interleave>
            <zeroOrMore>
               <text/>
            </zeroOrMore>
            <zeroOrMore>
               <element name="br">
                  <empty/>
               </element>
            </zeroOrMore>
         </interleave>
      </element>
   </define>
   <define name="e_VSpace">
      <element name="VSpace">
         <a:documentation>Create a vertically stretching space. The space has a minimum height of 0 but is able to stretch up to infinity. Useful in table cells.</a:documentation>
