   <define name="e_DefineColorprofile">
      <element name="DefineColorprofile">
         <a:documentation>Associate an external color profile with a name. To be used in PDFOptions.</a:documentation>
         <optional>
            <attribute name="condition">
               <a:documentation>Description of the output condition.</a:documentation>
            </attribute>
         </optional>
         <attribute name="colors">
            <a:documentation>Number of the colors in this color profile</a:documentation>
         </attribute>
         <attribute name="filename">
            <a:documentation>Filename of the color profile.</a:documentation>
         </attribute>
         <attribute name="identifier">
            <a:documentation>The official identifier for the registry.</a:documentation>
         </attribute>
         <attribute name="info">
            <a:documentation>Short text about the color profile.</a:documentation>
         </attribute>
         <attribute name="name">
            <a:documentation>The internal name of the color profile. To be used in PDFOptions.</a:documentation>
         </attribute>
         <optional>
            <attribute name="registry">
               <a:documentation>The name of the registry. Defaults to http://www.color.org/.</a:documentation>
            </attribute>
         </optional>
         <empty></empty>
      </element>
   </define>
   <define name="e_DefineColor">
      <element name="DefineColor">
         <a:documentation>Colors defined with DefineColors can be referenced later by their name.</a:documentation>
         <optional>
            <attribute name="colorname">
               <a:documentation>The name of the spot color if model is »spotcolor«. The name must match the required color name, such as »PANTONE 116 C«.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="model">
               <a:documentation>Color model to be used for the color. Currently “rgb”, “cmyk”, “gray” and “spotcolor” are supported.</a:documentation>
               <choice>
                  <value>cmyk</value>
                  <a:documentation>CMYK (cyan, magenta, yellow, key/black), values between 0 and 100 (100 = full intensity)</a:documentation>
                  <value>rgb</value>
                  <a:documentation>rgb (red, green, blue), values between 0 and 100, 100 means full intensity</a:documentation>
                  <value>RGB</value>
                  <a:documentation>rgb (red, green, blue), values between 0 and 255, 255 means full intensity</a:documentation>
                  <value>gray</value>
                  <a:documentation>Gray (0=black, 100=white)</a:documentation>
                  <value>spotcolor</value>
                  <a:documentation>Use a PANTONE or HKS color.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <attribute name="name">
            <a:documentation>The name of the color to be defined.</a:documentation>
         </attribute>
         <optional>
            <attribute name="overprint">
               <a:documentation>Enable overprint for this color.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation>Enable overprint for this color.</a:documentation>
                  <value>no</value>
                  <a:documentation>Disable overprint for this color (default).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="value">
               <a:documentation>Hex value of the color, such as #FA5 or #FFAA55.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="r">
               <a:documentation>Red part with RGB</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="g">
               <a:documentation>Green part with RGB / gray part when using the model gray</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="b">
               <a:documentation>Blue part with RGB</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="c">
               <a:documentation>Cyan part with CMYK</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="m">
               <a:documentation>Magenta part with CMYK</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="y">
               <a:documentation>Yellow part with CMYK</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="k">
               <a:documentation>Black part with CMYK</a:documentation>
            </attribute>
         </optional>
         <empty></empty>
      </element>
   </define>
   <define name="e_DefineFontalias">
      <element name="DefineFontalias">
         <a:documentation>Defines a font alias. When defining a font family, you can use the an alias for the fontname. Aliases are looked up recursively.</a:documentation>
         <attribute name="alias">
