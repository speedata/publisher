   <define name="e_LoadFontfile">
      <element name="LoadFontfile">
         <a:documentation>Load a font file (.otf, .ttf, .pfb) and associate it with an internal name.</a:documentation>
         <attribute name="name">
            <a:documentation>The internal name of the font file. To be used within DefineFontfamily.</a:documentation>
         </attribute>
         <attribute name="filename">
            <a:documentation>The name (with extension) of the font file.</a:documentation>
         </attribute>
         <optional>
            <attribute name="space">
               <a:documentation>The natural width between words. Can be stretched by 30% and shrunk by 10%. Defaults to 25. The value is a percentile of the font size.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="marginprotrusion">
               <a:documentation>The amount of protrusion glyphs like -, . and - stick into the right margin. Highly font dependent. Defaults to 0.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="smallcaps">
               <a:documentation>Use small caps glyphs when the font supplies them.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Use small caps for this font.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Don't switch to small caps (default).</a:documentation>
               </choice>
