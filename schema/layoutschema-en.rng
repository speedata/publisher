   <define name="e_Image">
      <element name="Image">
         <a:documentation>Includes an external Graphic. Allowed graphic formats are PDF (.pdf), PNG (.png) and JPEG (.jpg).</a:documentation>
         <optional>
            <attribute name="width">
               <a:documentation>Image width. One of 'auto' (default, take image width), '100%' (whole area width), length (such as '3cm') or number (in grid cells).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="height">
               <a:documentation>Image height. One of 'auto' (default, take image width), length (such as '3cm') or number (in grid cells).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="href">
               <a:documentation>An URL (http://....) of the image. Will be downloaded and cached in TEMPDIR/sp/images (which can be configured by setting the configuration file key imagecache) . Alternative to ›file‹.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="file">
               <a:documentation>Filename of the image. Can be a file in the search path or a file-URI for absolute paths (e.g. file:///path/to/image.pdf).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="page">
               <a:documentation>The page number from the PDF. Default is 1 (include the first page).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="naturalsize">
               <a:documentation>The PDF box that represents the visible area. Default is “cropbox”.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="maxwidth">
               <a:documentation>The maximum width of the image. Only used when clip="no". Value is a number (grid cells), a length or the value »100%« for full width image.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="maxheight">
               <a:documentation>The maximum height of the image. Only used when clip="no". Value is a number (grid cells) or a length.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="maxsize">
               <a:documentation>The PDF box that represents the maximum width. Default is “cropbox”.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="clip">
               <a:documentation>When yes, the image keeps its apsect ratio if both width and hight is given. To be able to fit the image into the given dimensions, the image gets clipped.</a:documentation>
               <choice>
                  <value>yes</value>
                  <a:documentation xmlns="">Keep the aspect ratio and cut off left/right or top/bottom edges.</a:documentation>
                  <value>no</value>
                  <a:documentation xmlns="">Distort the image to make it fit in the given area.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="minwidth">
               <a:documentation>The nimimum width of the image. Only used when clip="no". Value is a number (grid cells), a length or the value »100%« for full width image.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="minheight">
               <a:documentation>The minimum height of the image. Only used when clip="no". Value is a number (grid cells) or a length.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="dpiwarn">
               <a:documentation>Warn if the image gets lower resultion than given.</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
