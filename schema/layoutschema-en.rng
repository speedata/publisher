   <define name="e_Barcode">
      <element name="Barcode">
         <a:documentation>Print a 1d or 2d barcode. To be used in PlaceObject.</a:documentation>
         <attribute name="select">
            <a:documentation>The data to be encoded in the barcode.</a:documentation>
         </attribute>
         <optional>
            <attribute name="fontface">
               <a:documentation>Name of the fontface of the text that can be placed beneath the barcode. Not used in all codes.</a:documentation>
