   <define name="e_Column">
      <element name="Column">
         <a:documentation>Set the properties of a column in the table.</a:documentation>
         <optional>
            <attribute name="align">
               <a:documentation>The horizontal alignment of the table cells in this column. Can be overridden in a cell (Td).</a:documentation>
               <choice>
                  <value>left</value>
                  <a:documentation>The table cells are left aligned.</a:documentation>
                  <value>center</value>
                  <a:documentation>The table cells are horizontally centered.</a:documentation>
                  <value>right</value>
                  <a:documentation>The table cells are right aligned.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="backgroundcolor">
               <a:documentation>All cells in this column have this background color.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="padding-left">
               <a:documentation>Set the left padding for the column</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="padding-right">
               <a:documentation>Set the right padding for the column</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="width">
               <a:documentation>Width of the column. Argument can be a number (in grid cells) a length (e.g. 2cm) or a *-number (e.g. 4*).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="valign">
               <a:documentation>The vertical alignment of the cells in this column. Can be overridden in a cell (Td).</a:documentation>
               <choice>
                  <value>top</value>
                  <a:documentation>The table cells are top aligned.</a:documentation>
                  <value>middle</value>
                  <a:documentation>The table cells are vertically centered.</a:documentation>
                  <value>bottom</value>
                  <a:documentation>The table cells are aligned at the bottom.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty></empty>
      </element>
   </define>
   <define name="e_Columns">
